{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "globals": {
        "localStorage": true
    },
    "rules": {
        "react/jsx-no-bind": [
            2,
            {
                "allowArrowFunctions": true
            }
        ],
        "no-underscore-dangle": [
            "error",
            {
                "allow": [
                    "__REDUX_DEVTOOLS_EXTENSION__"
                ]
            }
        ]
    }
}